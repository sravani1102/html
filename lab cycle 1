a.

<html>
    <body>
        <form action="8a1.html">
            <fieldset>
                <label for="title"><p align="center"><b>BIO DATA</b></p></label>
                <label for="fname">First Name :</label>
                <input type="text" id="fname" required><br>
                <label for="lname">Last Name :</label>
                <input type="text" id="lname"><br>
                <label for="Gender :">Gender : </label><br>
                <input type="radio" id="female" value="Female">
                <label for="female">Female</label><br>
                <input type="radio" id="male" value="Male">
                <label for="male">Male</label><br>
                <label for="dob">Date of Birth(DD/MM/YY)</label>
                <input type="date" id="dob" required><br>
                <datalist id="caste">
                    <option value="General"></option>
                    <option value="OBC"></option>
                    <option value="BC"></option>
                    <option value="ST"></option>
                    <option value="SC"></option>
                </datalist>
                <label for="c">Caste:</label>
                <input list="caste"><br>
                <label for="hobbies">Hobbies :</label><br>
                <textarea name="hobbies" rows="5" cols="50"></textarea>
                <center><input type="submit"></fieldset></center>
            </fieldset>
        </form>
    </body>
</html>

