        <script>
            var l=document.anchors;
            document.write("<br>Number of anchor tags are "+l.length+"<br>");
            for(let i=0;i<l.length;i++)
            {
                document.write("The "+i+"th anchor tag is :"+l[i]+"<br>");
            }
        </script>
    </body>
</html>

