5.
a)
<!DOCTYPE html>
<html>
    <body>
        <script>
            var str=new String("Javascript String objects");
            document.write(str);
            document.write("<br>Length : "+str.length);
            document.write("<br>Uppercase : "+str.toUpperCase());
            document.write("<br>LowerCase : "+str.toLowerCase());
            document.write("<br>charat 5th index : "+str.charAt(5));
            document.write("<br>charcodeat 5th index : "+str.charCodeAt(5));
            document.write("<br>substring from index 3 to 10 : "+str.substring(3,10));
            document.write("<br>concat : "+str.concat(" ","using new keyword"));
            document.write("<br>Replace : "+str.replace('a','0'));
            var a=str.split('');
            document.write("<br>split : "+a);
            document.write("<br>startsWith : "+str.startsWith('c',5));
            document.write("<br>endsWith : "+str.endsWith('objects'));
            document.write("<br>search : "+str.search('script'));
        </script>
    </body>
</html>

b)
<!DOCTYPE html>
<html>
    <body>
        <script>
            document.write("Math object constants<br>");
            document.write("Math.E : "+Math.E+"<br>");
            document.write("Math.LN2 : "+Math.LN2+"<br>");
            document.write("Math.LN10 : "+Math.LN10+"<br>");
            document.write("Math.SQRT2 : "+Math.SQRT2+"<br>");
            document.write("Math.SQRT1_2 : "+Math.SQRT1_2+"<br>");
            document.write("Math.LOG2E : "+Math.LOG2E+"<br>");
            document.write("Math.LOG10E : "+Math.LOG10E+"<br>");
            document.write("Math object Methods<br>")
            document.write("Math.sqrt(10) : "+Math.sqrt(10)+"<br>");
            document.write("Math.random() : "+Math.random()+"<br>");
            document.write("Math.round(4.52) : "+Math.round(4.52)+"<br>");
            document.write("Math.floor(4.52) : "+Math.floor(4.52)+"<br>");
            document.write("Math.ceil(4.52) : "+Math.ceil(4.52)+"<br>");
            document.write("Math.trunc(4.52) : "+Math.trunc(4.52)+"<br>");
            document.write("Math.pow(3,4) : "+Math.pow(3,4)+"<br>");
            document.write("Math.min(32,12,435,12,10,-1) : "+Math.min(32,12,435,12,10,-1)+"<br>");
            document.write("Math.max(32,12,435,12,10,-1) : "+Math.max(32,12,435,12,10,-1)+"<br>");
        </script>
    </body>
</html>

c)

<!DOCTYPE html>
<html>
    <body>
        <p>Date Objects</p>
        <script>
            var date=new Date();
            document.write("Date : "+date.getDate());
            document.write("<br>utc Date : "+date.getUTCDate());
            document.write("<br>Time : "+date.getTime());
            document.write("<br>Year : "+date.getFullYear());
            document.write("<br>UTC Year : "+date.getUTCFullYear());
            document.write("<br>Month : "+date.getMonth());
            document.write("<br>UTC Month : "+date.getUTCMonth());
            document.write("<br>Day : "+date.getDay());
            document.write("<br>UTC Day : "+date.getUTCDay());
            document.write("<br>Hours : "+date.getHours());
            document.write("<br>UTC Hours : "+date.getUTCHours());
            document.write("<br>Minutes : "+date.getMinutes());
            document.write("<br>UTC Minutes : "+date.getUTCMinutes());
            document.write("<br>Seconds : "+date.getSeconds());
            document.write("<br>UTC Seconds : "+date.getUTCSeconds());
            document.write("<br>Milliseconds : "+date.getMilliseconds());
            document.write("<br>UTC Milliseconds : "+date.getUTCMilliseconds());
            document.write("<br>Set");
            var userdate=new Date(2022,08,08,12,0,0);
            document.write("<br>Date : ",userdate);
            document.write("<br>Time : "+userdate.setTime(1223342132));
            userdate.setFullYear(2002);
            document.write("<br>Year : "+userdate.getFullYear(1233));
            userdate.setMonth(11);
            document.write("<br>Month : "+userdate.getMonth());
            userdate.setDate(11);
            document.write("<br> Date : "+userdate.getDate());
            userdate.setHours(03);
            document.write("<br>Hours : "+userdate.getHours());
        </script>
    </body>
</html>

6.a)

<!DOCTYPE html>
<html>
    <body>
        <p>Hello</p>
        <h1>This is 6a labcycle question</h1>
        <script>
            var a=document.all;
            for(let i=0;i<a.length;i++)
            {
                document.write(a[i].tagName);
            }
        </script>
    </body>
</html>

6.b)
<!DOCTYPE html>
<html>
    <body>
        <ol id="mylist">
            <li>HTML</li>
            <li>CSS</li>
            <li>Javascript</li>
        </ol>
        <script>
            var l=document.getElementById('mylist');
            for(var i=0;i<l.children.length;i++)
            {
                document.write("The "+i+" children item is "+l.children[i].innerHTML+"<br>");
            }
        </script>
    </body>
</html>

